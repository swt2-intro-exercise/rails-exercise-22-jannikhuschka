<h2>Author index</h2>
 <table>
    <tr><th>Name</th><th>homepage</th></tr>
    <% @authors.each do |author| %>
        <tr>
            <td><%= author.name %></td>
            <td><%= author.homepage %></td>
            <td><%= link_to 'Show', author %></td>
            <td><%= link_to 'Edit', edit_author_path(author) %></td>
            <td><%= button_to 'Delete', author, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
</table> 
<%= link_to 'New', new_author_path %>
